@using BookLib.ViewModel
@model AuthorVM
@section Title {
Authors
}
@section subTitle {

Details
}

<div class="card-body">
	<table class="table table-striped">
		<thead>
			<tr>
				<th style="width: 10px">#</th>
				<th>Name</th>
				<th>
					Created At
				</th>
				<th>
					Updated At
				</th>

			</tr>
		</thead>
		<tbody>

			<tr>
				<td>@Model.Id</td>
				<td>
					@Model.Name
				</td>
				<td>
					@Model.CreatedOn
				</td>
				<td>
					@Model.UpdatedOn
				</td>
			</tr>

		</tbody>
	</table>
</div>



